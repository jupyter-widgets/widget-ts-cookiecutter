{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop":true,
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "outDir": "../lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2015"
  }
}
